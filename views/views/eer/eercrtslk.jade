html
	head
		style.
			.slklist a {
				font-size:2em;
				color:black;
				text-decoration:none;
			}
	body
		div(id='back')
			a(href='./eer?stchk=0') Create Slab and Block
		div
			div(id='forming')
				form(action='/admin/eerget',method='get')
					a SLK Info
					input(type='text',name='slkinfo')
					input(type='hidden',name='eermode', value=0)
					input(type='radio',name='modimode', value=0 checked)
					|Slab
					input(type='radio',name='modimode', value=1)
					|Block
					input(type='submit', value='submit')
		div(id='slblist')
			div(class='slklist')
				a SLAB LIST
			div
				-for(var ia=0; ia<slab.length; ia++){
					div
						a(href='./eer?stchk=2&slabid='+slab[ia].inhid)=slab[ia].slabinfo
				-}
		div(id='blocklist')
			div(class='slklist')
				a BLOCK LIST
			div
				-for(var ib=0; ib<block.length; ib++){
					div
						a=block[ib].blockinfo
				-}

